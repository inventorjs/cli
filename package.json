    "eslint": "^8.21.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@inventorjs/cli": "^1.3.4"
  }
